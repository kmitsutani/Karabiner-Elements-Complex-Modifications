          "to": {"key_code": "6", "modifiers": ["shift"]}
        },
        {
          "type": "basic",
          "from": {"key_code": "8", "modifiers": {"mandatory": ["shift"], "optional": ["command", "control", "option"]}},
          "to": {"key_code": "quote", "modifiers": ["shift"]}
        },
        {
          "type": "basic",
          "from": {"key_code": "9", "modifiers": {"mandatory": ["shift"], "optional": ["command", "control", "option"]}},
